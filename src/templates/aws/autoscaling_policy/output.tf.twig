# Define list of variables to be output

output "arn" {
  value = "${aws_autoscaling_policy.{{ name }}.arn}"
}

output "name" {
  value = "${aws_autoscaling_policy.{{ name }}.name}"
}

output "autoscaling_group_name" {
  value = "${aws_autoscaling_policy.{{ name }}.autoscaling_group_name}"
}

output "adjustment_type" {
  value = "${aws_autoscaling_policy.{{ name }}.adjustment_type}"
}

output "policy_type" {
  value = "${aws_autoscaling_policy.{{ name }}.policy_type}"
}
