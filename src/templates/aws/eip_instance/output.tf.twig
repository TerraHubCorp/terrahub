# Define list of variables to be output

output "id" {
  value = "${aws_eip.{{ name }}.id}"
}

output "private_ip" {
  value = "${aws_eip.{{ name }}.private_ip}"
}

output "public_ip" {
  value = "${aws_eip.{{ name }}.public_ip}"
}

output "instance" {
  value = "${aws_eip.{{ name }}.instance}"
}

output "network_interface" {
  value = "${aws_eip.{{ name }}.network_interface}"
}

output "associate_with_private_ip" {
  value = "${aws_eip.{{ name }}.associate_with_private_ip}"
}
