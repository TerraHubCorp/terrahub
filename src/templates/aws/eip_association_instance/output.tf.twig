# Define list of variables to be output

output "association_id" {
  value = "${aws_eip_association.{{ name }}.association_id}"
}

output "allocation_id" {
  value = "${aws_eip_association.{{ name }}.allocation_id}"
}

output "instance_id" {
  value = "${aws_eip_association.{{ name }}.instance_id}"
}

output "network_interface_id" {
  value = "${aws_eip_association.{{ name }}.network_interface_id}"
}

output "private_ip_address" {
  value = "${aws_eip_association.{{ name }}.private_ip_address}"
}

output "public_ip" {
  value = "${aws_eip_association.{{ name }}.public_ip}"
}
